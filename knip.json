{
	"$schema": "https://unpkg.com/knip@latest/schema.json",
	"ignoreExportsUsedInFile": { "interface": true, "type": true },
	"workspaces": {
		"emoji-blast": {
			"entry": ["src/index.ts!", "src/mains/*.ts!"],
			"project": ["src/**/*.ts!"]
		},
		"konami-emoji-blast": {
			"entry": ["src/index.ts!", "src/mains/*.ts!"],
			"project": ["src/**/*.ts!"]
		},
		"konami-emoji-blast-astro": {
			"entry": ["src/index.ts!"],
			"project": ["src/**/*.ts!"]
		},
		"konami-emoji-blast-react": {
			"entry": ["src/index.ts!"],
			"project": ["src/**/*.ts!", "src/**/*.tsx!"]
		},
		"konami-emoji-blast-typedoc": {
			"entry": ["src/index.ts!"],
			"project": ["src/**/*.ts!"]
		}
	}
}
