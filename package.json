{
  "name": "emojisplosion",
  "version": "2.6.0",
  "description": "Blasts emoji like fireworks all up in your HTML page.",
  "main": "src/index.js",
  "scripts": {
    "tsc": "tsc",
    "eslint": "eslint src/**/*.ts --report-unused-disable-directives",
    "verify": "npm run tsc && npm run eslint & npm run webpack",
    "watch": "chokidar \"src/**/*.ts\" -i \"src/**/*.d.ts\" -c \"npm run webpack\"",
    "webpack": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joshuakgoldberg/emojisplosion.git"
  },
  "keywords": [
    "emoji",
    "explosion",
    "fireworks",
    "page",
    "all",
    "up",
    "in",
    "there",
    "dom"
  ],
  "author": "Josh Goldberg <joshuakgoldberg@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joshuakgoldberg/emojisplosion/issues"
  },
  "homepage": "https://github.com/joshuakgoldberg/emojisplosion#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "chokidar": "^3.4.1",
    "chokidar-cli": "^2.1.0",
    "eslint": "^7.5.0",
    "requirejs": "2.3.6",
    "run-p": "0.0.0",
    "ts-loader": "^8.0.1",
    "typescript": "^3.9.7",
    "webpack": "^4.44.0",
    "webpack-cli": "^3.3.12"
  }
}
